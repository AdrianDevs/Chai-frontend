{
  "name": "chai-frontend",
  "private": true,
  "version": "0.1.0",
  "description": "Web frontend for the Chai chat app",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "typecheck": "tsc --noEmit",
    "gitleaks": "gitleaks detect --verbose --source ."
  },
  "keywords": [
    "vite",
    "react",
    "typescript"
  ],
  "author": "AdrianDevs",
  "license": "ISC",
  "validate-branch-name": {
    "pattern": "^(master|main|develop){1}$|^(feature|fix|hotfix|release|text|experimental)/.+$",
    "errorMsg": "Branch name validation failed"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.0.8",
    "@tanstack/react-form": "^0.45.1",
    "@tanstack/react-router": "^1.111.7",
    "axios": "^1.8.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "tailwindcss": "^4.0.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@eslint/js": "^9.19.0",
    "@tailwindcss/typography": "^0.5.16",
    "@tanstack/config": "^0.16.3",
    "@tanstack/router-devtools": "^1.111.7",
    "@tanstack/router-plugin": "^1.111.7",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "daisyui": "^5.0.0-beta.8",
    "eslint": "^9.19.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "prettier": "^3.5.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.22.0",
    "validate-branch-name": "^1.3.2",
    "vite": "^6.1.0"
  }
}
